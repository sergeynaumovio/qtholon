#!/usr/bin/qtloader6

[HolonOpenTasks]
class = HolonOpenTasks

[Holon]
class = HolonDesktop
mainWindowState = QByteArray()
minimumHeight = 200
minimumWidth = 400
sidebarAreaList = QStringList(Left, Right, Top, Bottom)
sidebarAreasMovableShortcut = Alt+M
sidebarKeyboardModifier = Qt::AltModifier
sidebarList = QCharList(1, 2, 3, 4, 5, 6, 7, 8, 9, 0)
sidebarWidgets = QStringList(HolonOpenTasks)
styleSheet = QMainWindow::separator { width: 1px; height: 1px; background-color: rgb(34, 36, 38); }
titleBarHeight = 10
titleBarStyleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); }

[Holon/Taskbar]
class = HolonTaskbar
area = bottom
preferedHeight = 40
preferedWidth = 50
styleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); }

[Holon/Taskbar/Menu]
class = HolonMenu
styleSheet = QPushButton { border: 0px; } QPushButton:hover { background-color: rgb(34, 36, 38); }

[Holon/Taskbar/SidebarSwitch]
class = HolonSidebarSwitch

[Holon/Sidebar1]
class = HolonSidebar
sidebar = QChar(1)
sidebarArea = Left

[Holon/Sidebar1/OpenTasks1]
class = HolonOpenTasks

[Holon/Sidebar1/OpenTasks2]
class = HolonOpenTasks

[Holon/Sidebar2]
class = HolonSidebar
sidebar = QChar(2)
sidebarArea = Top
