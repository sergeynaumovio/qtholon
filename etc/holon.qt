#!/usr/bin/qtloader6

[holon]
class = HolonDesktop
buttonStyleSheet = QPushButton { border: 0px; } QPushButton:hover { background-color: rgb(74, 76, 78); color: white; }
externalMainWindowState = QByteArray(AAAA/wAAAAD9AAAAAgAAAAAAAADIAAACuvwCAAAAAfwAAAAAAAACugAAAHMA///+/AEAAAAC+wAAAAoAdABhAHMAawBzAQAAAAAAAADIAAAAyAD////7AAAAFABwAGEAcgBhAG0AZQB0AGUAcgBzAQAAAMkAAADIAAAAAAAAAAAAAAABAAAAyAAAArr8AgAAAAH7AAAAEABzAGUAdAB0AGkAbgBnAHMBAAAAAAAAAroAAABzAP///gAAARUAAAK6AAAABAAAAAQAAAAIAAAACPwAAAAA)
internalMainWindowState = QByteArray(AAAA/wAAAAD9AAAAAQAAAAMAAAEVAAAAc/wBAAAAAfsAAAAQAG0AZQBzAHMAYQBnAGUAcwEAAAAAAAABFQAAAAAAAAAAAAABFQAAAroAAAAEAAAABAAAAAgAAAAI/AAAAAA=)
menuStyleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); border: 1px solid rgb(34, 36, 38); }
menuWidth = 180
minimumHeight = 200
minimumWidth = 400
saveShortcut = Ctrl+S
sidebarMoveShortcut = Alt+M
sidebarSwitchButtonWidth = 120
styleSheet = QMainWindow::separator { width: 1px; height: 1px; background-color: rgb(34, 36, 38); }
taskbarArea = bottom
taskbarPreferedHeight = 26
taskbarPreferedWidth = 50
taskbarStyleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); }
titleBarHeight = 24
titleBarStyleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); }

[holon/task-model]
class = HolonTaskModel

[holon/task-model/branch-a]
class = HolonTaskModelBranch
title = Branch A

[holon/task-model/branch-a/custom-task1]
class = HolonCustomTask
title = Custom Task A

[holon/task-model/branch-b]
class = HolonTaskModelBranch
title = Branch B

[holon/task-model/branch-b/custom-task2]
class = HolonCustomTask
title = Custom Task B

[holon/workflow-model1]
class = HolonWorkflowModel

[holon/workflow-model1/branch1]
class = HolonWorkflowModelBranch

[holon/task-tree-window]
class = HolonTaskTreeWindow

[holon/task-list-window]
class = HolonTaskListWindow

[holon/terminal-window]
class = HolonTerminalWindow

[holon/task-tree]
class = HolonSidebar
checked = true
group = tasks
layout = external
shortcut = Alt+1
title = Project Tasks

[holon/task-tree/task-tree-window]
class = HolonTaskTreeWindow

[holon/task-list]
class = HolonSidebar
checked = false
group = tasks
layout = external
shortcut = Alt+2
title = Open Tasks

[holon/task-list/task-list-window]
class = HolonTaskListWindow

[holon/parameters]
class = HolonSidebar
checked = false
layout = external
shortcut = Alt+3
title = Parameters

[holon/workflows]
class = HolonWindowArea
checked = false
shortcut = Alt+9
title = Workflows

[holon/workflows/workflow-window]
class = HolonWorkflowWindow

[holon/messages]
class = HolonSidebar
checked = false
layout = internal
shortcut = Alt+0
title = Messages

[holon/settings]
class = HolonSidebar
checked = true
layout = external
shortcut = Alt+-
title = Settings

[holon/shell]
class = QLoaderShell
home = holon/data

[holon/shell/cd]
class = QLoaderShellCd

[holon/shell/clear]
class = QLoaderShellClear

[holon/shell/exit]
class = QLoaderShellExit

[holon/shell/save]
class = QLoaderShellSave

[holon/data]
class = QLoaderData

[holon/storage]
class = QLoaderStorage
