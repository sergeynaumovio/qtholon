#!/usr/bin/qtloader6

[Holon]
class = HolonDesktop
buttonStyleSheet = QPushButton { border: 0px; } QPushButton:hover { background-color: rgb(74, 76, 78); color: white; }
mainWindowState = QByteArray()
minimumHeight = 200
minimumWidth = 400
newWindowMenuStyleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); border: 1px solid rgb(34, 36, 38); }
newWindowMenuWidth = 180
sidebarAreaList = QStringList(Left, Right, Top, Bottom)
sidebarAreasMovableShortcut = Alt+M
sidebarKeyboardModifier = Qt::AltModifier
sidebarList = QCharList(1, 2, 3, 4, 5, 6, 7, 8, 9, 0)
styleSheet = QMainWindow::separator { width: 1px; height: 1px; background-color: rgb(34, 36, 38); }
titleBarHeight = 24
titleBarStyleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); }

[Holon/OpenTasksWindow]
class = HolonOpenTasksWindow

[Holon/Taskbar]
class = HolonTaskbar
area = bottom
preferedHeight = 40
preferedWidth = 50
styleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); }

[Holon/Taskbar/Menu]
class = HolonNewTaskMenu

[Holon/Taskbar/SidebarSwitch]
class = HolonSidebarSwitch

[Holon/Sidebar(1)]
class = HolonSidebar
sidebar = QChar(1)
sidebarArea = Left

[Holon/Sidebar(1)/OpenTasksWindow(1)]
class = HolonOpenTasksWindow

[Holon/Sidebar(1)/OpenTasksWindow(2)]
class = HolonOpenTasksWindow

[Holon/Sidebar(2)]
class = HolonSidebar
sidebar = QChar(2)
sidebarArea = Top
