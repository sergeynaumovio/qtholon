[holon/new-tasks-model]
class = HolonNewTasksModel

[holon/new-tasks-model/dir-a]
class = HolonNewTasksDir
title = Dir A

[holon/new-tasks-model/dir-a/custom-task1]
class = HolonCustomTask
title = Custom Task

[holon/new-tasks-model/dir-b]
class = HolonNewTasksDir
title = Dir B

[holon/new-tasks-model/dir-b/custom-task2]
class = HolonCustomTask
title = Custom Task

[holon/open-tasks-model1]
class = HolonOpenTasksModel

[holon/open-tasks-model1/branch1]
class = HolonOpenTasksDir

[holon/new-tasks-window]
class = HolonNewTasksWindow

[holon/open-tasks-window]
class = HolonOpenTasksWindow

[holon/terminal-window]
class = HolonTerminalWindow

[holon/new-tasks]
class = HolonSidebar
checked = true
group = tasks
layout = external
shortcut = Alt+1
title = New Tasks

[holon/new-tasks/new-tasks-window]
class = HolonNewTasksWindow

[holon/open-tasks]
class = HolonSidebar
checked = false
group = tasks
layout = external
shortcut = Alt+2
title = Open Tasks

[holon/open-tasks/open-tasks-window]
class = HolonOpenTasksWindow

[holon/parameters]
class = HolonSidebar
checked = false
layout = external
shortcut = Alt+3
title = Parameters

[holon/parameters/open-tasks-window]
class = HolonOpenTasksWindow

[holon/workflows]
class = HolonWindowArea
checked = false
shortcut = Alt+9
title = Workflows

[holon/workflows/workflows-window]
class = HolonWorkflowsWindow

[holon/messages]
class = HolonSidebar
checked = false
layout = internal
shortcut = Alt+0
title = Messages

[holon/messages/open-tasks-window]
class = HolonOpenTasksWindow

[holon/settings]
class = HolonSidebar
checked = true
layout = external
shortcut = Alt+-
title = Settings

[holon/settings/open-tasks-window]
class = HolonOpenTasksWindow

[holon/shell]
class = QLoaderShell
home = QStringList(Holon, Data)

[holon/shell/cd]
class = QLoaderShellCd

[holon/shell/clear]
class = QLoaderShellClear

[holon/shell/exit]
class = QLoaderShellExit

[holon/shell/save]
class = QLoaderShellSave

[holon/data]
class = QLoaderData

[holon/storage]
class = QLoaderStorage
