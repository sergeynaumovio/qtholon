#!/usr/bin/qtloader6

[Holon]
class = HolonDesktop
buttonStyleSheet = QPushButton { border: 0px; } QPushButton:hover { background-color: rgb(74, 76, 78); color: white; }
externalMainWindowState = QByteArray()
internalMainWindowState = QByteArray(AAAA/wAAAAD9AAAAAQAAAAMAAACnAAAAaPwBAAAAAfsAAAAQAE0AZQBzAHMAYQBnAGUAcwEAAAAAAAAApwAAAKcA////AAAApwAAAlEAAAAEAAAABAAAAAgAAAAI/AAAAAA=)
menuStyleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); border: 1px solid rgb(34, 36, 38); }
menuWidth = 180
minimumHeight = 200
minimumWidth = 400
saveShortcut = Ctrl+S
sidebarMoveShortcut = Alt+M
sidebarSwitchButtonWidth = 120
styleSheet = QMainWindow::separator { width: 1px; height: 1px; background-color: rgb(34, 36, 38); }
taskbarArea = bottom
taskbarPreferedHeight = 26
taskbarPreferedWidth = 50
taskbarStyleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); }
titleBarHeight = 24
titleBarStyleSheet = QWidget { color: white; background-color: rgb(54, 56, 58); }

[Holon/OpenTasksWindow]
class = HolonOpenTasksWindow

[Holon/New Task]
class = HolonSidebar
checked = false
group = Tasks
layout = external
shortcut = Alt+1
title = New Task

[Holon/New Task/OpenTasksWindow]
class = HolonOpenTasksWindow

[Holon/Open Tasks]
class = HolonSidebar
checked = true
group = Tasks
layout = external
shortcut = Alt+2
title = Open Tasks

[Holon/Open Tasks/OpenTasksWindow]
class = HolonOpenTasksWindow

[Holon/Parameters]
class = HolonSidebar
checked = true
layout = external
shortcut = Alt+3
title = Parameters

[Holon/Parameters/OpenTasksWindow]
class = HolonOpenTasksWindow

[Holon/Messages]
class = HolonSidebar
checked = true
layout = internal
shortcut = Alt+4
title = Messages

[Holon/Messages/OpenTasksWindow]
class = HolonOpenTasksWindow

[Holon/Settings]
class = HolonSidebar
checked = true
layout = external
shortcut = Alt+0
title = Settings

[Holon/Settings/OpenTasksWindow]
class = HolonOpenTasksWindow

[Holon/Data]
class = QLoaderData

[Holon/Storage]
class = QLoaderStorage
